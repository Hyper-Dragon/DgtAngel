      <AutoGen>True</AutoGen>
      <DependentUpon>Moves-en-01.resx</DependentUpon>
    </Compile>
    <Compile Update="Assets\Speech-en-01.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Speech-en-01.resx</DependentUpon>
    </Compile>
    <Compile Update="Assets\Time-en-01.Designer.cs">
      <DesignTime>True</DesignTime>
