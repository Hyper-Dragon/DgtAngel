      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Time-en-01.Designer.cs</LastGenOutput>
    </EmbeddedResource>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Assets\Audio\Moves-en-01\" />
    <Folder Include="Assets\Audio\Time-en-01\" />
  </ItemGroup>
  
