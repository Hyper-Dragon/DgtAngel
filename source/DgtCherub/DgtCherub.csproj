    <None Remove="Assets\Audio\Speech-en-01\DgtCantFindBoard-AP.wav" />
    <None Remove="Assets\Audio\Speech-en-01\DgtConnected-AP.wav" />
    <None Remove="Assets\Audio\Speech-en-01\DgtDisconnected-AP.wav" />
    <None Remove="Assets\Audio\Speech-en-01\DgtLcConnected-AP.wav" />
    <None Remove="Assets\Audio\Speech-en-01\DgtLcDisconnected-AP.wav" />
